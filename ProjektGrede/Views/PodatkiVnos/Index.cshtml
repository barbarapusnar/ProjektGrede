@model IEnumerable<ProjektGrede.Models.PodatkiVnos>
@{
    ViewBag.Title = "Vnos";
}
<div class="container text-center">
    <div class="row">
          <div class="card text-white mb-3" style="background-color:wheat">
              <div class="card-body">
                  <h3> Padavine v obdobju od @ViewBag.ZadnjiDatum.ToString("dd/MM/yyyy") do @DateTime.Now.ToString("dd/MM/yyyy") so bile @ViewBag.ArsoPadavine mm.</h3>
                  <p class="card-text">Podatki so pridobljeni iz spletne strani Agencije RS za okolje.</p>
              </div>
          </div>
    </div>
    <div class="row row-eq-height" id="vnosCont">
        <div id="live">
            @Html.Partial("_postajeVnos", Model)
        </div>
    </div>
</div>
